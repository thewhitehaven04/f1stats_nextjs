        "fetch-api": "node ./src/client/apiGenerator/saveSpec.mjs",
        "update-api": "openapi-ts"
    },
    "dependencies": {
        "@prisma/client": "6.6.0",
        "@tailwindcss/typography": "0.5.15",
        "@tanstack/react-query": "5.66.11",
        "@tanstack/react-table": "^8.20.5",
        "clsx": "^2.1.1",
        "concurrently": "9.1.2",
        "country-flag-icons": "^1.5.18",
        "daisyui": "4.12.13",
        "date-fns": "4.1.0",
        "next": "15.2.0",
        "prisma": "6.5.0",
        "react": "^19.0.0",
        "react-dom": "^19.0.0",
        "react-icons": "5.5.0"
    },
    "devDependencies": {
        "@biomejs/biome": "^1.9.3",
        "@hey-api/client-fetch": "0.8.2",
        "@hey-api/openapi-ts": "0.64.8",
        "@types/node": "^20",
        "@types/react": "^19.0.10",
        "@types/react-dom": "^19.0.4",
        "node": "23.1.0",
        "postcss": "^8",
        "tailwindcss": "^3.4.1",
        "typescript": "^5"
    }
