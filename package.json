{
    "name": "f1stats",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "concurrently \"npm run next-dev\" \"npm run fastapi-dev\"",
        "build": "next build",
        "start": "next start",
        "lint": "next lint",
        "fastapi-dev": "pip3 install -r requirements.txt && cd api/ && python3 -m uvicorn main:app --reload",
